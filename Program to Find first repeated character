//Program to Find first repeated character
// input String = AMITRAVINDRADESALE
fun main() {
  val result = findrepeatCharacter("AMITRAVINDRADESALE")
  println("repeating character is $result")
}

fun findrepeatCharacter(s :  String ) : String {
    
    for (i in 0..s.length - 1) {
        
        for(j in 0..s.length - 1){
            if(s.get(i) == s.get(j)){
                
                return s.get(i).toString()
            }
        }
        
    }
    
    
    
    return ""
}
